{
  "private": true,
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "Simple React starter template",
  "scripts": {
    "build": "webpack",
    "build:dev": "NODE_ENV=development webpack",
    "fmt": "prettier --write \"{*,src/**/*,tools/**/*}.+(js|mjs|jsx|css|yml|json)\"",
    "fmt:check": "prettier --check \"{*,src/**/*,tools/**/*}.+(js|mjs|jsx|css|yml|json)\"",
    "lint": "eslint \"{*,src/**/*,tools/**/*}.+(js|mjs|jsx)\"",
    "start": "NODE_ENV=development webpack-dev-server",
    "test:unit": "jest",
    "test": "npm run fmt:check && npm run lint && npm run build && npm run test:unit"
  },
  "author": "Stefan Cameron",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/eslint-plugin": "^7.22.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.6.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-transform-stub": "^2.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "^3.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
